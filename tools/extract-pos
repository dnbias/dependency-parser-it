#!/bin/bash

## _ _ word _ pos _ _ _ _ _
## use like:
## ../tools/extract-pos it_isdt-ud-test.conllu
name="heldout.pos"
while read -a array; do
    # for key in "${!array[@]}"
    # do
    #     echo "${array[$key]}"
    # done
    if [ "${array[0]}" != "#" ]; then
        echo "${array[2]} ${array[4]}" >> $name
    fi
done <$1
